# copy file
scp

# remote
ssh user@host.ip.address

# generate new pub key
ssh-keygen

# copy ssh key between linux
ssh-copy-id -i $HOME/.ssh/id_rsa.pub user@server1.whatever

# copy ssh pub key to remote linux device
type $env:USERPROFILE\.ssh\id_rsa.pub | ssh shubow@172.24.242.48 "cat >> .ssh/authorized_keys"
